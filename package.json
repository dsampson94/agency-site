{
  "name": "superstacksolutions",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev",
    "dev:clean": "rimraf .next && prisma generate && next dev",
    "dev:turbo": "prisma generate && next dev --turbo",
    "build": "prisma generate && next build",
    "build:clean": "rimraf .next && prisma generate && next build",
    "build:analyze": "ANALYZE=true prisma generate && next build",
    "start": "prisma generate && next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rimraf .next .turbo node_modules/.cache",
    "reset": "npm run clean && npm install && npm run dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "preinstall": "npx only-allow npm"
  },
  "type": "module",
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.775.0",
    "@aws-sdk/credential-providers": "^3.775.0",
    "@leafac/rehype-shiki": "^2.2.1",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^14.0.4",
    "@prisma/client": "^6.15.0",
    "@types/bcryptjs": "^2.4.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "acorn": "^8.10.0",
    "acorn-jsx": "^5.3.2",
    "autoprefixer": "^10.4.7",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.0",
    "contxt": "^1.4.4",
    "escape-string-regexp": "^5.0.0",
    "fast-glob": "^3.2.12",
    "form-data": "^4.0.2",
    "framer-motion": "^10.15.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongodb": "^6.15.0",
    "mongoose": "^8.13.0",
    "next": "15.1.11",
    "openai": "^5.20.1",
    "prisma": "^6.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recma-import-images": "0.0.3",
    "remark-gfm": "^4.0.0",
    "remark-rehype-wrap": "0.0.3",
    "remark-unwrap-images": "^4.0.0",
    "shiki": "^0.11.1",
    "tailwindcss": "^3.4.1",
    "unified-conditional": "0.0.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "rimraf": "^6.0.1",
    "sharp": "0.33.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "prisma": {
    "seed": "node prisma/seed.cjs"
  }
}
