# Database
DATABASE_URL="postgresql://username:password@your-rds-endpoint:5432/your-database-name?schema=public"

# Example for AWS RDS PostgreSQL:
# DATABASE_URL="postgresql://superstack_user:your_password@superstack-db.cluster-xxxxx.us-east-1.rds.amazonaws.com:5432/superstack_db?schema=public"

# OpenAI API Key (for voice transcription)
OPENAI_API_KEY=your_openai_api_key_here

# NextAuth Secret (if using authentication)
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# AWS Configuration (if needed for S3 or other services)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
